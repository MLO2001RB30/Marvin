{
  "compilerOptions": {
    "target": "ES2021",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@pia/shared": [
        "packages/shared/src"
      ],
      "@pia/shared/*": [
        "packages/shared/src/*"
      ]
    }
  }
}
