PORT=4000
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
OPENAI_API_KEY=your-openai-api-key
OPENAI_BASE_URL=https://openrouter.ai/api/v1
OPENAI_MODEL=anthropic/claude-sonnet-4.5
# Optional: Fixed context injected into every assistant request (product info, guidelines, etc.)
# MARVIN_FIXED_CONTEXT=Marvin is a personal productivity app. Key features: calendar, email, Slack integration.
OPENAI_AUDIO_MODEL=gpt-4o-mini-transcribe
OAUTH_BASE_URL=https://your-api-host.example.com
OAUTH_APP_CALLBACK_URL=[REDACTED]
OAUTH_ENCRYPTION_KEY=replace-with-32-byte-base64-or-hex-key
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GOOGLE_REDIRECT_URI=https://your-api-host.example.com/v1/integrations/google/callback
SLACK_CLIENT_ID=your-slack-client-id
SLACK_CLIENT_SECRET=your-slack-client-secret
SLACK_REDIRECT_URI=https://your-api-host.example.com/v1/integrations/slack/callback
MICROSOFT_CLIENT_ID=your-microsoft-client-id
MICROSOFT_CLIENT_SECRET=your-microsoft-client-secret
MICROSOFT_REDIRECT_URI=https://your-api-host.example.com/v1/integrations/onedrive/callback
DROPBOX_CLIENT_ID=your-dropbox-client-id
DROPBOX_CLIENT_SECRET=your-dropbox-client-secret
DROPBOX_REDIRECT_URI=https://your-api-host.example.com/v1/integrations/dropbox/callback
PIPELINE_AUTO_RUN=false
PIPELINE_TICK_MS=300000
